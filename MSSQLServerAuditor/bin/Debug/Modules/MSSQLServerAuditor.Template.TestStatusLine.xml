					<PeriodTimeUnit>Minute</PeriodTimeUnit>
					<PeriodTimeUnitCount>1</PeriodTimeUnitCount>
					<StartingAt>PT00H00M05S</StartingAt>
					<EndingAt>PT23H59M59S</EndingAt>
				</DailyFrequency>

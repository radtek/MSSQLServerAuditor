				<parameter name="@boolIsShowLockedProcessesOnly" value="True"/>
			</sql-select>

			<RefreshSchedules>
				<RefreshSchedule>
					<Enabled>false</Enabled>
					<Id>001</Id>
					<Name>GetInstanceLockedProcesses</Name>
